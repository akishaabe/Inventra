    "outDir": "./out-tsc/app",
    "types": [],
  "isolatedModules": false,
  "allowImportingTsExtensions": false,
  "module": "ESNext",
  "moduleResolution": "Bundler",
    "baseUrl": "."
  },
  "include": [
